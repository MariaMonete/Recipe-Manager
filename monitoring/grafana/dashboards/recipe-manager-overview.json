{
  "id": null,
  "uid": "rm-overview",
  "title": "Recipe Manager - Overview",
  "tags": ["recipe-manager", "observability"],
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "time": { "from": "now-24h", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"] },
  "panels": [
    {
      "type": "stat",
      "title": "Ingredient Count (current)",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "recipe_manager_ingredient_count",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" } ] }
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "graphMode": "none",
        "colorMode": "value"
      },
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "Ingredient Count over time",
      "id": 2,
      "datasource": { "type": "prometheus", "uid": "Prometheus" },
      "targets": [
        {
          "refId": "A",
          "expr": "recipe_manager_ingredient_count",
          "legendFormat": "count"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "decimals": 0,
          "custom": { "drawStyle": "line", "lineWidth": 2, "fillOpacity": 10 }
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "list", "placement": "bottom" },
        "tooltip": { "mode": "single" }
      },
      "gridPos": { "h": 10, "w": 16, "x": 8, "y": 0 }
    }
  ],
  "annotations": {
    "list": [
      {
        "type": "dashboard",
        "name": "Annotations & Alerts",
        "enable": true,
        "hide": false
      }
    ]
  }
}

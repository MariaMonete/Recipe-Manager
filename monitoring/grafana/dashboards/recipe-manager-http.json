{
  "id": null,
  "uid": "rm-http",
  "title": "Recipe Manager HTTP & RabbitMQ",
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-3h", "to": "now" },
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP Request Rate (by status)",
      "id": 1,
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (status) (rate(http_server_requests_seconds_count[5m]))"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "HTTP Latency p95 (s)",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(http_server_requests_seconds_bucket[5m])))"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
    },
    {
      "type": "timeseries",
      "title": "RabbitMQ Messages Published/Consumed",
      "id": 3,
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(rabbitmq_queue_messages_published_total[5m]))"
        },
        {
          "refId": "B",
          "expr": "sum(rate(rabbitmq_queue_messages_consumed_total[5m]))"
        }
      ],
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 8 }
    }
  ]
}

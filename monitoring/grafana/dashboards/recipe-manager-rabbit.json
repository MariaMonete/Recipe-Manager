{
  "id": null,
  "uid": "rm-rabbit",
  "title": "Recipe Manager â€“ RabbitMQ",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": { "from": "now-6h", "to": "now" },
  "tags": ["rabbitmq", "prometheus"],
  "templating": {
    "list": [
      {
        "name": "queue",
        "type": "query",
        "datasource": "Prometheus",
        "refresh": 1,
        "hide": 0,
        "includeAll": true,
        "multi": true,
        "query": "label_values(rabbitmq_queue_messages_ready, queue)"
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Connections",
      "id": 1,
      "datasource": "Prometheus",
      "targets": [{ "refId": "A", "expr": "sum(rabbitmq_connections)" }],
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Channels",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [{ "refId": "A", "expr": "sum(rabbitmq_channels)" }],
      "gridPos": { "h": 5, "w": 6, "x": 6, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Consumers (all queues)",
      "id": 3,
      "datasource": "Prometheus",
      "targets": [{ "refId": "A", "expr": "sum(rabbitmq_queue_consumers)" }],
      "gridPos": { "h": 5, "w": 6, "x": 12, "y": 0 }
    },
    {
      "type": "stat",
      "title": "Ready msgs (all queues)",
      "id": 4,
      "datasource": "Prometheus",
      "targets": [{ "refId": "A", "expr": "sum(rabbitmq_queue_messages_ready)" }],
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 0 }
    },

    {
      "type": "timeseries",
      "title": "Queue depth (ready) by queue",
      "id": 5,
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (queue) (rabbitmq_queue_messages_ready{queue=~\"$queue\"})"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 5 }
    },
    {
      "type": "timeseries",
      "title": "Unacked by queue",
      "id": 6,
      "datasource": "Prometheus",
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (queue) (rabbitmq_queue_messages_unacked{queue=~\"$queue\"})"
        }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 5 }
    },

    {
      "type": "timeseries",
      "title": "Published vs Consumed (rate)",
      "id": 7,
      "datasource": "Prometheus",
      "targets": [
        { "refId": "A", "expr": "sum(rate(rabbitmq_queue_messages_published_total[5m]))" },
        { "refId": "B", "expr": "sum(rate(rabbitmq_queue_messages_consumed_total[5m]))" }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 13 }
    },
    {
      "type": "timeseries",
      "title": "Consumers per queue",
      "id": 8,
      "datasource": "Prometheus",
      "targets": [
        { "refId": "A", "expr": "sum by (queue) (rabbitmq_queue_consumers{queue=~\"$queue\"})" }
      ],
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 13 }
    }
  ]
}
